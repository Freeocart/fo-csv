{{ header }}
{{ column_left }}

<div id="content">

  <div class="page-header">
    <div class="container-fluid">
      <div class="pull-right">
        <a href="" data-toggle="tooltip" title="" class="btn btn-default" data-original-title="Отменить"><i class="fa fa-reply"></i></a></div>

      <h1>{{ heading_title }}</h1>
      <ul class="breadcrumb">
      {% for breadcrumb in breadcrumbs %}
        <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
      {% endfor %}
      </ul>
    </div>
  </div>

  <div class="container-fluid">
    <div id="foc_csv">
      <div class="container">
        <div class="row">
          <noscript>
            <div class="col-sm-6 col-sm-offset-3">
              <div class="panel panel-danger">
                <div class="panel-heading">
                  <span class="panel-title">{{ foc_app_preload_nojs }}</span>
                </div>
              </div>
            </div>
          </noscript>
          <div class="col-sm-6 col-sm-offset-3">
            <div class="panel panel-default">
              <div class="panel-heading">
                <span class="panel-title">{{ heading_title }}: {{ foc_version }}</span>
              </div>
              <div class="panel-body">
                <p>
                  {{ foc_app_preload_description }}
                </p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript">
// Frontend app settings
window.FOC_CSV_PARAMS = {
  'requestConfig' : {
    'token': '{{ token }}',
    'tokenName': '{{ tokenName }}',
    'baseRoute': '{{ baseRoute }}',
    'baseUrl': '{{ baseUrl }}',
  },
  'appName' : '{{ heading_title }}',
  'appVersion' : '{{ foc_version }}',
  'language': '{{ language }}',
  'initial' : {{ initial }}
};
</script>
{% for script in scripts %}
  <script src="{{ script }}" type="text/javascript"></script>
{% endfor %}

{{ footer }}